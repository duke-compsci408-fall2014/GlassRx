package com.compsci408.rxcore.datatypes;

public enum AccountType {
	
	PATIENT(0, "Patient", MainActivity.class),
	PROVIDER(1, "Provider");

	private final int mId;
	private final String mName;
	
	AccountType(int id, String name, Activity activity) {
		mId = id;
		mName = name;
		mActivity = activity;
	}

	public int getId() {
		return mId;
	}

	public String getName() {
		return mName;
	}
}
